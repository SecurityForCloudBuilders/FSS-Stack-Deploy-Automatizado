{
    "Version": "2012-10-17",
    "Statement": [
    {
            "Sid": "CopyFromScanningBucket",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:DeleteObject",
                "s3:GetObjectTagging"
            ],
            "Resource": "arn:aws:s3:::fss-demo-01/*"
        },
        {
            "Sid": "CopyToPromoteOrQuarantineBucket",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectTagging",
                "s3:PutObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::demolab-fss-quarantine/*",
                "arn:aws:s3:::demolab-fss-promote/*"
            ]
        }
    ]
}